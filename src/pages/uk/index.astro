---
import Container from '@/components/Container.astro'
import BaseLayout from '@/layouts/BaseLayout.astro'
import AboutSteps from '@/components/AboutSteps.astro'
import AboutUsHero from '@/components/AboutUsHero.astro'
import PartnersApp from '@/components/PartnersApp.astro'
import MainPage from '@/components/layout/MainPage.astro'
import PartnersCompany from '@/components/PartnersCompany.astro'
import PartnersPush from '@/components/PartnersPush.astro'
import Testimonials from '@/components/services/Testimonials.astro'

export const prerender = true
---

<BaseLayout
  title="AquaPumpers - Надаємо професійні послуги з монтажу, технічного обслуговування та ремонту водонасосних пристроїв та водяних свердловин у Києві та Київській області! | Безкоштовна консультація зараз!"
  description="Потрібні надійне встановлення та ремонт? або послуги з технічного обслуговування вашої свердловини та систем каналізації? Наша команда фахівців гарантує першокласний сервіс у Києві та Київській області. Замовляйте зараз, щоб отримати ефективні та доступні рішення!"
>
  <MainPage />
  <Container>
    <AboutSteps />
    <PartnersApp />
    <Testimonials />
    <AboutUsHero />
    <PartnersCompany />
    <PartnersPush />
  </Container>
</BaseLayout>
