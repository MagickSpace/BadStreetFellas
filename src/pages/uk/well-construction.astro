import BaseLayout from '@/layouts/BaseLayout.astro'
import ServicesMwells from '@/components/mwells/ServicesMwells.astro'
import ServicesGMwells from '@/components/mwells/ServicesGMwells.astro'
import LogoHeroMwells from '@/components/mwells/LogoHeroMwells.astro'
